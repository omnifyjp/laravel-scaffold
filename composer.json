    "minimum-stability": "dev",
    "type": "library",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "OmnifyJP\\LaravelScaffold\\": "src/"
        },
        "files": [
            "helpers/common.php"
        ]
    },
    "authors": [
        {
            "name": "Pham Thai Duong",
            "email": "26842626+ecsol@users.noreply.github.com"
        }
